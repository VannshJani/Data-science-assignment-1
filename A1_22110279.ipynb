    "import tqdm\n",
    "from tqdm import trange\n",
    "\n",
    "def euclidean_distance(a, b):\n",
    "    \"\"\"Compute Euclidean distance between two vectors.\"\"\"\n",
    "    return np.linalg.norm(a - b)\n",
    "\n",
    "def single_linkage_clustering(X, k):\n",
    "    \"\"\"\n",
    "    Perform single linkage agglomerative clustering on data X until only k clusters remain.\n",
    "    \n",
    "    Parameters:\n",
    "        X (np.ndarray): Data matrix of shape (n_samples, n_features).\n",
    "        k (int): Desired number of clusters.\n",
    "        \n",
    "    Returns:\n",
    "        labels (np.ndarray): Cluster labels for each data point (array of length n_samples).\n",
    "    \"\"\"\n",
    "    n = X.shape[0]\n",
    "    # Start with each point as its own cluster: clusters is a list of lists containing point indices.\n",
    "    clusters = [[i] for i in range(n)]\n",
    "    \n",
    "\n",
    "    dist_matrix = np.full((n, n), np.inf)\n",
    "    for i in range(n):\n",
    "        for j in range(i+1, n):\n",
    "            d = euclidean_distance(X[i], X[j])\n",
    "            dist_matrix[i, j] = d\n",
    "            dist_matrix[j, i] = d\n",
    "\n",
    "    while len(clusters) > k:\n",
    "        print(\"Number of clusters:\", len(clusters))\n",
    "        min_distance = np.inf\n",
    "        merge_pair = (None, None)\n",
    "        \n",
    "        # Find the two clusters with the smallest single linkage distance.\n",
    "        for i in range(len(clusters)):\n",
    "            for j in range(i+1, len(clusters)):\n",
    "                # Compute distance between clusters[i] and clusters[j]\n",
    "                current_min = np.inf\n",
    "                for idx_i in clusters[i]:\n",
    "                    for idx_j in clusters[j]:\n",
    "                        current_min = min(current_min, dist_matrix[idx_i, idx_j])\n",
    "                if current_min < min_distance:\n",
    "                    min_distance = current_min\n",
    "                    merge_pair = (i, j)\n",
    "        \n",
    "        i, j = merge_pair\n",
    "        new_cluster = clusters[i] + clusters[j]\n",
    "        clusters.pop(j)\n",
    "        clusters.pop(i)\n",
    "        clusters.append(new_cluster)\n",
    "\n",
    "    labels = np.empty(n, dtype=int)\n",
    "    for cluster_label, cluster in enumerate(clusters):\n",
    "        for idx in cluster:\n",
    "            labels[idx] = cluster_label\n",
    "    return labels\n",
