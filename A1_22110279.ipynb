    "\n",
    "def preprocess_images(images, normalize=True):\n",
    "    \"\"\"\n",
    "    Preprocess the MNIST images by flattening each image to a vector.\n",
    "    \n",
    "    Parameters:\n",
    "        images (np.ndarray): Array of shape (num_images, rows, cols).\n",
    "        normalize (bool): If True, normalize pixel values to [0, 1].\n",
    "        \n",
    "    Returns:\n",
    "        np.ndarray: A 2D array of shape (num_images, rows*cols).\n",
    "    \"\"\"\n",
    "    n_samples = images.shape[0]\n",
    "    # Flatten each image (28x28 becomes 784)\n",
    "    flattened = images.reshape(n_samples, -1).astype(np.float32)\n",
    "    if normalize:\n",
    "        flattened /= 255.0\n",
    "    return flattened\n",
    "\n",
    "\n",
    "images_file = 't10k-images.idx3-ubyte'\n",
