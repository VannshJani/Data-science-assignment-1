    "from sklearn.metrics import rand_score\n",
    "\n",
    "n = flattened_images.shape[0]\n",
    "\n",
    "def kmeans_plus_plus(X, k):\n",
    "    '''\n",
    "    X is the data matrix of shape (n_samples, n_features)\n",
    "    k is the number of clusters\n",
    "    '''\n",
    "    n_samples = X.shape[0]\n",
    "    centroids = np.empty((k, X.shape[1]), dtype=X.dtype)\n",
    "    # Choose the first centroid uniformly at random.\n",
    "    index = np.random.randint(0, n_samples)\n",
    "    centroids[0] = X[index]\n",
    "    \n",
    "    # Initialize a vector for distances (squared distances)\n",
    "    distances = np.linalg.norm(X - centroids[0], axis=1)**2\n",
    "    \n",
    "    for i in range(1, k):\n",
    "        # Compute probabilities proportional to the squared distances\n",
    "        probs = distances / np.sum(distances)\n",
    "        next_index = np.random.choice(n_samples, p=probs)\n",
    "        centroids[i] = X[next_index]\n",
    "        # Update the distances: for each point, keep the minimum distance\n",
    "        new_distances = np.linalg.norm(X - centroids[i], axis=1)**2\n",
    "        distances = np.minimum(distances, new_distances)\n",
    "    \n",
    "    return centroids\n",
    "\n",
    "\n",
    "def lloyd_kmeans(X, k, max_iters=1000, tol=1e-6):\n",
    "    # Initialize centroids using kmeans++ initialization\n",
    "    centroids = kmeans_plus_plus(X, k)\n",
    "    \n",
    "    for it in range(max_iters):\n",
    "        # Compute distances from each point to each centroid\n",
    "        # The result is an (n_samples x k) matrix.\n",
    "        distances = np.linalg.norm(X[:, np.newaxis] - centroids, axis=2)\n",
    "        # Assign each point to the nearest centroid\n",
    "        labels_pred = np.argmin(distances, axis=1)\n",
    "        \n",
    "        new_centroids = np.array([\n",
    "            X[labels_pred == i].mean(axis=0) if np.any(labels_pred == i) else centroids[i]\n",
    "            for i in range(k)\n",
    "        ])\n",
    "        \n",
    "        if np.linalg.norm(new_centroids - centroids) < tol:\n",
    "            break\n",
    "        \n",
    "        centroids = new_centroids\n",
    "        \n",
    "    return labels_pred, centroids\n",
    "\n",
    "# Run k-means clustering with k = 10\n",
    "k = 10\n",
    "predicted_labels, centroids = lloyd_kmeans(flattened_images, k)\n",
    "\n",
    "rand_index = rand_score(labels, predicted_labels)\n",
    "print(\"Rand Index:\", rand_index)\n"
