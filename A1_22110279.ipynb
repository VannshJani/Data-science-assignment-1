    "def greedy_k_center(X, k):\n",
    "    \"\"\"\n",
    "    Implements the greedy k-center clustering algorithm.\n",
    "    \n",
    "    Parameters:\n",
    "        X (np.ndarray): Data matrix of shape (n_samples, n_features).\n",
    "        k (int): Number of clusters (centers).\n",
    "        \n",
    "    Returns:\n",
    "        predicted_labels (np.ndarray): Cluster label for each point.\n",
    "        centers (np.ndarray): Array of shape (k, n_features) with the chosen centers.\n",
    "    \"\"\"\n",
    "    n_samples = X.shape[0]\n",
    "    centers = []\n",
    "    # Randomly choose the first center.\n",
    "    first_index = np.random.randint(0, n_samples)\n",
    "    centers.append(X[first_index])\n",
    "    \n",
    "    # For each subsequent center, choose the point farthest from the current centers.\n",
    "    for i in range(1, k):\n",
    "        # Compute the distance from each point to its nearest center.\n",
    "        distances = np.full(n_samples, np.inf)\n",
    "        for center in centers:\n",
    "            dist = np.linalg.norm(X - center, axis=1)**2  # squared distances\n",
    "            distances = np.minimum(distances, dist)\n",
    "        next_index = np.argmax(distances)\n",
    "        centers.append(X[next_index])\n",
    "    \n",
    "    centers = np.array(centers)\n",
    "    \n",
    "    # After centers are chosen, assign each point to the nearest center.\n",
    "    distances_matrix = np.linalg.norm(X[:, np.newaxis] - centers, axis=2)  # shape: (n_samples, k)\n",
    "    predicted_labels = np.argmin(distances_matrix, axis=1)\n",
    "    \n",
    "    return predicted_labels, centers\n",
    "\n",
    "\n",
    "k = 10\n",
    "predicted_labels, centers = greedy_k_center(flattened_images, k)\n",
    "\n",
    "ri = rand_score(labels, predicted_labels)\n",
    "print(\"Rand Index:\", ri)\n"
   ]
